	"sets": [
		{
			"name": "Glass Cannon Attacker",
			"items": [
				"Beedrillite"
			],
			"nature":[
				"Jolly"
			],
			"ability": "Adaptability",
			"evs": [
				"252 Attack",
				"252 Speed",
				"4 Special Defense"
			],
			"moves": [
				"Protect",
				"X-Scissor",
